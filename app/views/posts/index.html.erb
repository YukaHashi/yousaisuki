<h1>Posts#index</h1>
<% @posts.each do |post| %>
  <div>
    <%= image_tag post.get_image %>
    <p>投稿画像:<%= image_tag 'sample-author1.jpg' %></p>
    <p>タイトル:<%= post.title %></p>
    <p>本文：<%= post.body %></p>
    <p>ユーザーネーム:<%= post.user.name %></p>
  </div>
<% end %>
<p>Find me in app/views/posts/index.html.erb</p>
